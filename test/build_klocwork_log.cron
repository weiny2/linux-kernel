#!/bin/bash
# To subscribe to these notices enter your email below and check-in the change.
build=`git rev-parse --short HEAD`
grep "failed" klocwork.*.log
if [ $? -eq 0 ]; then
        mailx -S smtp=smtp://smtp.intel.com -s "[CRON_REG] klocwork check for build $build FAILED" dennis.dalessandro@intel.com < klocwork.*.log
        mailx -S smtp=smtp://smtp.intel.com -s "[CRON_REG] klocwork check for build $build FAILED" jubin.john@intel.com < klocwork.*.log
else
        mailx -S smtp=smtp://smtp.intel.com -s "[CRON_REG] klocwork check for build $build SUCCESS" dennis.dalessandro@intel.com < klocwork.*.log
fi
