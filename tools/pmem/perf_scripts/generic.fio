[global]
filename=/dev/btt0
bs=1m
ioengine=sync
iodepth=2
direct=1
norandommap
time_based=1
runtime=30
group_reporting
disable_lat=1
disable_slat=1
disable_clat=1
clat_percentiles=0
numjobs=8
unlink=1
#verify=crc32-intel
#verify_fatal=1
#verify_async=4

[mixed]
rw=randrw
stonewall

[write]
rw=randwrite
stonewall

[read]
rw=randread
stonewall
